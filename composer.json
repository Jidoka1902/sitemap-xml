{
    "name": "jidoka1902/sitemap-xml",
    "description": "a little library to provide basic php interface defined by https://www.sitemaps.org/protocol.html",
    "type": "library",
    "require": {
        "php": "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.11.6"
    },
    "autoload": {
        "psr-4": {
            "Jidoka1902\\SitemapXML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jidoka1902\\SitemapXML\\Tests\\": "tests/"
        }
    },
